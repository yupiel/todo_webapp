{
  "name": "todo_webapp",
  "version": "1.0.0",
  "description": "TODO Webapp written in NodeJS bundled with Webpack.",
  "private": true,
  "scripts": {
    "dev": "webpack --config ./webpack.dev.js",
    "serve-dev": "webpack serve --config ./webpack.dev.js",
    "prod": "webpack --config ./webpack.prod.js",
    "serve-prod": "webpack serve --config ./webpack.prod.js",
    "heroku-postbuild": "webpack --config ./webpack.server.js && webpack --config ./webpack.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yupiel/todo_webapp.git"
  },
  "author": "Stefan Mitrovic <@Yupiel>",
